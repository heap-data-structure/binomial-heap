{
  "name": "@heap-data-structure/binomial-heap",
  "description": "Binomial heap data structures for JavaScript",
  "version": "11.0.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://heap-data-structure.github.io/binomial-heap",
  "repository": {
    "url": "https://github.com/heap-data-structure/binomial-heap",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/heap-data-structure/binomial-heap/issues"
  },
  "keywords": [
    "binomial",
    "data",
    "heap",
    "javascript",
    "js",
    "structure"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "install-hooks": "husky install",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "13.1.0",
    "@heap-data-structure/specification": "16.0.0",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.1.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.7.3",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "7.0.1",
    "np": "6.5.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.9",
    "xo": "0.42.0"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "defaults",
            "maintained node versions"
          ]
        }
      ]
    ],
    "env": {
      "debug": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ]
      },
      "cover": {
        "sourceMaps": "both",
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "current node"
            }
          ],
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ]
      },
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
